page{
  background-color: #f1f1f1;
}
.main{

}
.route{
  padding: 0 20rpx;
}
.class{
  padding: 0 20rpx;
}
.route .title{
  display: block;
  text-align: center;
  width: 100%;
  height: 100rpx;
  background-color: #1296db;
  color: #fff;
  line-height: 100rpx;
}
.route .name{
  display: block;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 20rpx;
  text-align: center;
  background-color: #fff;
  border-bottom: 3rpx #f1f1f1 solid;
}
.route .info{
  display: inline-block;
  width: 100%;
  line-height: 80rpx;
  padding:  0  20rpx;
  background-color: #fff;
  border-bottom: 3rpx #f1f1f1 solid;
}
.class .title{
  display: block;
  text-align: center;
  width: 100%;
  height: 100rpx;
  background-color: #1296db;
  color: #fff;
  line-height: 100rpx;
}
.class .photo{
  display: block;
  width: 100%;
  height: 400rpx;
  border-bottom: 3rpx #f1f1f1 solid;
}
.class .name{
  font-weight: bold;
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #fff;
  text-align: center;
}
.startstudy{
  display: inline-block;
  position: relative;
  width: 100%;
}
.startstudy .btn{
  position: absolute;
  height: 100rpx;
  width: 50%;
  left: 25%;
  top: 50rpx;
  background-color: #1296db;
  color: #fff;
  font-weight: bold;
  text-align: center;
  line-height: 100rpx;
  font-size: 35rpx;
  border-radius: 10rpx;
}

.main1{

}
.video{
  display: block;
  width: 100%;
  padding: 0 25rpx;
}
.dwn{
  display: block;
  background: #f1f1f1;
  padding: 20rpx;
  width: 100%;
}
.dwn .title{
  display: inline-block;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.dwn .left{
  float: left;
  left: 20rpx;
  font-weight: bold;
  font-size: 30rpx;
}
.dwn .right{
  float: right;
  right: 20rpx;
}
.dwn .index{
  display: inline-block;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  padding: 10rpx 0rpx;
}
.dwn .index .end{
  float: right;
  color: #bbb9b9;
  font-size: 25rpx;
  font-weight: bold;
}