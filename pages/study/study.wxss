page{
  background: url('https://www.api.yyhelper.icu/static/icons/study-bg.jpg') no-repeat;
  background-size:100% 100%;
  background-attachment:fixed;
}
.title{
  display: block;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  font-weight: bold;
  font-size: 35rpx;
  text-align: center;
}
.right{
  display: block;
  width: 100%;
  height: 100rpx;
  position: relative;
}
.right .icons{
  position: absolute;
  left: 67%;
  top: 0rpx;
  text-align: center;
}
.right .info{
  position: absolute;
  left: 50%;
  top: 80rpx;
  color: #fff;
  font-weight: bold;
  font-size: 35rpx;
  width: 50%;
  text-align: center;
}
.left{
  display: block;
  width: 100%;
  height: 100rpx;
  position: relative;
}
.left .icons{
  position: absolute;
  left: 17%;
  top: 0;
  text-align: center;
}
.left .info{
  position: absolute;
  left: 0;
  top: 80rpx;
  font-weight: bold;
  font-size: 35rpx;
  color: #fff;
  width: 50%;
  text-align: center;
}

.deletebtn{
  margin-top: 200rpx;
  display: block;
  width: 100%;
  padding: 0 20%;
  height: 100rpx;
  line-height: 100rpx;
}
.close{
  width: 100%;
  background-color: rgba(7, 7, 207, 0.418);
  text-align: center;
  font-weight: bold;
  font-size: 35rpx;
  color: #fff;
}