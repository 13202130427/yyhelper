.main{

}
.search{
  display: inline-block;
  position: fixed;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  border-bottom: 2rpx #e1e1e1 solid;
  z-index: 10;
}
.search-image{
  float: left;
  margin-top: 25rpx;
}
.search-input{
  float: left;
  margin-left: 20rpx;
  height: 100rpx;
}
.search-close{
  float: right;
  margin-top: 25rpx;
  margin-right: 20rpx;
}
.nav{
  display: block;
  z-index: 20;
}
.title{
  display: block;
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  border-bottom: 2rpx #f1f1f1 solid;
  position: relative;
}

.title-text{
  position: absolute;
  left: 10rpx;
}
.title-image{
  position: absolute;
  right: 20rpx;
  top: 33rpx;
}
.info{
  top:246rpx;
  position: fixed;
  width: 100%;
  z-index: 100
}
.info-mask{
  width: 10%;
  height: 1200rpx;
  float: left;
  background-color: rgba(26, 26, 26, 0.425);
}
.info-main{
  width: 90%;
  height: 1200rpx;
  float: left;
  background-color: #f1f1f1;
  position: relative
}
.info-left{
  width: 50%;
}
.left-list{
  float: left;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center
}
.info-right{
  position: absolute;
  width: 50%;
  background-color: #fff;
  top: 0rpx;
  right: 0rpx;
  height: 1200rpx;
  
}
.right-list{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center
}
.search-down{
  display: block;
  width: 100%;
}
.result{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx #f1f1f1 solid;
  margin-left: 60rpx;
}